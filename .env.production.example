# Keycloak Configuration for Production
# Copie este arquivo para .env.production e configure com seus valores reais

# URL do Keycloak em produção (HTTPS obrigatório)
KEYCLOAK_URL=https://keycloak.suaempresa.com

# Realm do Keycloak
KEYCLOAK_REALM=vehicle-sales

# Client ID
KEYCLOAK_CLIENT_ID=vehicle-sales-app

# Credenciais do admin do Keycloak (MUDE ESTAS CREDENCIAIS!)
KEYCLOAK_ADMIN_USER=admin
KEYCLOAK_ADMIN_PASS=SUA_SENHA_SUPER_SEGURA_AQUI

# Client Secret (será obtido automaticamente pelo script)
KEYCLOAK_CLIENT_SECRET=

# Configurações de segurança
KEYCLOAK_SSL_REQUIRED=external
KEYCLOAK_VERIFY_EMAIL=true
KEYCLOAK_REGISTRATION_ALLOWED=false

# URLs válidas para produção
KEYCLOAK_VALID_REDIRECT_URIS=https://app.suaempresa.com/*,https://admin.suaempresa.com/*
KEYCLOAK_WEB_ORIGINS=https://app.suaempresa.com,https://admin.suaempresa.com

# Configurações de token
KEYCLOAK_ACCESS_TOKEN_LIFESPAN=3600
KEYCLOAK_REFRESH_TOKEN_LIFESPAN=86400 
